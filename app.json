{
  "expo": {
    "name": "ADM",
    "slug": "ADM",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#01bf62"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afghanbyteTechnologies.ADM"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.afghanbyteTechnologies.ADM"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],

    "scheme": "com.afghanbyteTechnologies.ADM",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d3058b04-69a8-48fc-9c24-73b45272dbe0"
      }
    }
  }
}
